
<article class="card__section">
    <div class="card__image">
    <img src="<%= image %>" alt="<%= title %>">

    </div>

    <div class="card__section-div">
            <h3>
                <a href="/products/<%= id %>">
                    <%= title %>
                </a>
            </h3>
            <p><%= price %> </p>
            <p class="card__section-puntos">PUNTOS</p>

    </div>
</article>
